<!DOCTYPE html>
    <title>create - page</title>
<%- include("./partials/head.ejs") %>

<body>
    <%- include("./partials/nav.ejs") %>
    <h2>All Blogs</h2>
    <% if (blogs.length > 0) { %>
        <% blogs.forEach(blog => { %>
            <h3><%= blog.title %></h3>
            <p><%= blog.content %></p>
        <% }) %>
    <% } else { %>
        <p>No blogs to display</p>
    <% } %>
    
</body>
</html>